const encodeUriComponent = require('encodeUriComponent');
const queryPermission = require('queryPermission');
const getCookieValues = require('getCookieValues');
const getReferer = require('getReferrerUrl');
const logger = require('logToConsole');
const injectScript = require('injectScript');

// init the Conversify object and set the api_key
const Conversify = [];
Conversify.api_key = data.apiKey;

// get user id from cookie
const cookieName = '__conversify_uid';
let cookieValues;
if (queryPermission('get_cookies', cookieName)) {
  var uid = getCookieValues(cookieName)[0];
} else {
  var uid = '';
}

// get the referer
const getReferrerUrl = require('getReferrerUrl');
let referrer;
if (queryPermission('get_referrer', 'query')) {
  referrer = getReferrerUrl('queryParams');
} else {
  referrer = '';
}

// shorthand for encodeUriComponent
var e = encodeUriComponent;

// try to get product data
var p=Conversify.product&& Conversify.product.id?Conversify.product.id:'';

// build url for Conversify script
var cfy_script ='//js.conversify.com/v2/'+e(Conversify.api_key)+'.js?pt='+e(Conversify.pagetype||'')+'&ut='+e(uid||'')+'&rf='+e(referrer||'')+'&p='+e(p||'');

// finally inject script
injectScript(cfy_script, data.gtmOnSuccess, data.gtmOnFailure, cfy_script);


